/*Programa de divisao*/
programa
{
	funcao inicio()
	{
		/*Declaracao de variaveis*/
		real n1, n2, divi
		/*Entrada de valores das variaveis*/
		escreva("Bem vindo ao programa de divisoa.\n\n")
		escreva("Digite o primeiro numero: ")
		leia(n1)
		escreva("Digite o segundo numero: ")
		leia(n2)
		/*Bloco de comando*/
		divi = n1 / n2
		/*Saida de valor divi*/
		escreva("\nO valor da divisao e ", divi)
	}
}
